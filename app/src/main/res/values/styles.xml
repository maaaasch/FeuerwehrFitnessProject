<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="CustomManuel">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CustomManuel" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimary">@color/primary_material_light_1</item>
        <item name="android:colorBackground">@color/background_material_light</item>
        <item name="android:textColorPrimary">@android:color/background_light</item>
        <item name="android:textColorHint">@android:color/background_light</item>
    </style>

</resources>
